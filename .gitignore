# Generic ones
.DS_Store
.idea
local.*
*.sql
.swp
.vscode

# Ignore directories generated by composer
/drush/contrib/
/vendor/
/docroot/core/
/docroot/modules/contrib/
/docroot/themes/contrib/
/docroot/profiles/contrib/
/docroot/libraries/

# Ignore Drupal's local setting overrides
docroot/sites/default/settings.local.php

# Ignore local development services
docroot/sites/sites/development.services.yml
docroot/sites/sites/default/settings.private.php

# Ignore paths that contain user-generated content.
docroot/files
docroot/sites/*/files
docroot/sites/*/private

# Docker
.persist
web

# Ignore built frontend assets, these get built in pipelines
docroot/themes/custom/fsa/dist
docroot/themes/custom/fsa/node_modules
/.gitattributes

# Make sure core upgrades don't remove Acquia .htaccess rules.
docroot/.htaccess