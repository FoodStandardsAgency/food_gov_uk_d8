<?php

/**
 * @file
 * Contains fsa_managed_links.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function fsa_managed_links_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the fsa_managed_links module.
    case 'help.page.fsa_managed_links':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides centrally managed link library') . '</p>';
      return $output;

    default:
  }
}
