{#
/**
 * @file
 * Default node template override to display a teaser.
 *
 * @see template_preprocess_node()
 */
#}
<a href="{{ url }}" rel="bookmark">
  <div{{ attributes }}>
    <h4{{ title_attributes }}>{{ label }}</h4>
    <div{{ content_attributes }}>
      {{ content }}
    </div>
  </div>
</a>